    <property name="version">0.0.1</property>
    <property name="copyright" translatable="yes">Copyright 2010
Julien 'Lta' BALLET &lt;elthariel@gmail.com&gt;</property>
    <property name="website">http://github.com/eltharial/kiara</property>
    <property name="website_label" translatable="yes">Code repository on github</property>
